<SCRIPT TYPE="TEXT/JAVASCRIPT">function onTimeout(){
var href = "https://encore.concordia.ca:443/iii/encore/logoutFilterRedirect?suite=def";
self.document.location.href = href;
}
var timeoutTimer = null;
function renewSession() {
clearInterval(timeoutTimer);
timeoutTimer = setTimeout(onTimeout, 10800000);}
window.onload = function() {
renewSession();
}
