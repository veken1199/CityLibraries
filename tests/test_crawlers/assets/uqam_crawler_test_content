  $(window).load(function() {
		$( "span.EXLFfacebookIframePlaceholder").each(function() {
				var url = $(this).find('input').val();
			    var f = jQuery('<iframe>');
			    f.attr('src',url);
			    f.attr('title',"Facebook Like");
			    f.attr('scrolling',"no");
