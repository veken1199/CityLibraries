var populateValuestocDisplay = function populateValuestocDisplay(data, textStatus) {
 for (var i = 0; i < data.length; i++ ) {
            var elemId = data[i].id;
            var escId = elemId.replace(/\//g,'\\/').replace(/\(/g, '\\(').replace(/\)/g, '\\)').replace(/\./g,'\\.');
                displayTocLink(escId, data[i].response);
 }
}

