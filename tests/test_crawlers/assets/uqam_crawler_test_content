var populateValuescitationTrails = function populateValuescitationTrails(data, textStatus) {
 for (var i = 0; i < data.length; i++ ) {
            var elemId = data[i].id;
            var escId = elemId.replace(/\//g,'\\/').replace(/\(/g, '\\(').replace(/\)/g, '\\)').replace(/\./g,'\\.');
                displayCitationTrails(escId, data[i].response);
 }
}

